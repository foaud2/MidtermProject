                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="600+ item Sold Out"
                        android:textColor="@color/white"
                        android:layout_marginTop="8dp" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/line3"
                android:layout_width="195dp"
                android:layout_height="380dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="8dp"
                android:background="@color/TealBlue"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line2">

                <ImageView
                    android:id="@+id/im5_3"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:src="@drawable/imgsearch2"
                    android:scaleType="centerCrop" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="8dp">

                    <TextView
                        android:id="@+id/tv5_12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Woman"
                        android:textColor="@color/white"
                        android:layout_marginTop="10dp" />

                    <TextView
                        android:id="@+id/tv5_13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="$50"
                        android:textColor="@color/white"
                        android:layout_marginTop="8dp" />

                    <TextView
                        android:id="@+id/tv5_14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="T-Shirt 2"
                        android:textSize="15dp"
                        android:textColor="@color/white"
                        android:layout_marginTop="8dp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:orientation="horizontal"
                        android:layout_marginTop="8dp">

                        <TextView
                            android:id="@+id/tv5_15"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:text="(5.0)"
                            android:layout_gravity="center"
                            android:textColor="@color/green" />

                        <RatingBar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:numStars="5"
                            android:rating="4.5"
                            android:scaleX="0.7"
                            android:scaleY="0.7" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv5_16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="600+ item Sold Out"
                        android:textColor="@color/white"
                        android:layout_marginTop="8dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/line4"
                android:layout_width="195dp"
                android:layout_height="380dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="10dp"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line1">

                <ImageView
                    android:id="@+id/im5_17"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:src="@drawable/imgsearch1"
                    android:scaleType="centerCrop" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="8dp">

