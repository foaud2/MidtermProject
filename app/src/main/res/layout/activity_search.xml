<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/f"
    tools:context=".search">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/SearchView"
        app:layout_constraintBottom_toTopOf="@+id/icon5_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:id="@+id/line1"
                android:layout_width="195dp"
                android:layout_height="380dp"
                android:layout_marginLeft="8dp"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/im5_1"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:src="@drawable/imgsearch1"
                    android:scaleType="centerCrop" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="8dp">

                    <TextView
                        android:id="@+id/tv5_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Woman"
                        android:layout_marginTop="10dp" />

                    <TextView
                        android:id="@+id/tv5_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="$100"
                        android:textColor="@color/TealBlue"
                        android:layout_marginTop="8dp" />

                    <TextView
                        android:id="@+id/tv5_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="T-Shirt 1"
                        android:textSize="15dp"
                        android:layout_marginTop="8dp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="8dp">

                        <TextView
                            android:id="@+id/tv5_5"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:text="(5.0)"
                            android:layout_gravity="center"
                            android:textColor="@color/green" />

                        <RatingBar
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:numStars="5"
                            android:rating="4.5"
                            android:scaleX="0.7"
                            android:scaleY="0.7" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv5_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="600+ item Sold Out"
                        android:layout_marginTop="8dp" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/line2"
                android:layout_width="195dp"
                android:layout_height="380dp"
                android:layout_marginRight="8dp"
                android:background="@color/TealBlue"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/im5_2"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:src="@drawable/imgsearch2"
                    android:scaleType="centerCrop" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="8dp">

                    <TextView
                        android:id="@+id/tv5_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Woman"
                        android:textColor="@color/white"
                        android:layout_marginTop="10dp" />

                    <TextView
                        android:id="@+id/tv5_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="$50"
                        android:textColor="@color/white"
                        android:layout_marginTop="8dp" />

                    <TextView
                        android:id="@+id/tv5_9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="T-Shirt 2"
                        android:textSize="15dp"
                        android:textColor="@color/white"
                        android:layout_marginTop="8dp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:orientation="horizontal"
                        android:layout_marginTop="8dp">

                        <TextView
                            android:id="@+id/tv5_10"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:text="(5.0)"
                            android:layout_gravity="center"
                            android:textColor="@color/green" />

                        <RatingBar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:numStars="5"
                            android:rating="4.5"
                            android:scaleX="0.7"
